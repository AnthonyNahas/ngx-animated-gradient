    "animations",
    "background",
    "colors",
    "gradient"
  ],
  "bugs": {
    "url": "https://github.com/AnthonyNahas/ngx-animated-gradient/issues"
  },
  "private": true,
  "scripts": {
    "start": "gulp build",
    "test": " gulp test",
    "demo": " gulp build:demo",
