{
  "name": "nx-animated-gradient",
  "description": "Angular Directive that animated the gradient background",
  "version": "0.0.1",
  "homepage": "https://github.com/anthonynahas/ngx-animated-gradient",
  "author": {
    "name": "Anthony Nahas",
    "url": "https://github.com/anthonynahas"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/anthonynahas/ngx-animated-gradient.git"
  },
  "license": "MIT",
  "keywords": [
    "ngx",
    "library",
    "angular",
    "animations",
    "background",
    "colors",
    "gradient"
  ],
  "bugs": {
    "url": "https://github.com/anthonynahas/ngx-animated-gradient/issues"
  },
  "private": true,
  "scripts": {
    "start": "gulp build",
    "test": " gulp test",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=anthonynahas/ngx-animated-gradient --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/core": "^7.2.9",
    "@angular/common": "^7.2.9"
   },
  "devDependencies": {
    "@angular/compiler" : "^7.2.9",
    "@angular/platform-server" : "^7.2.9",
    "@angular/platform-browser" : "^7.2.9",
    "@angular/platform-browser-dynamic" : "^7.2.9",
    "@angular/compiler-cli" : "^7.2.9",
    "zone.js" : "0.8.26",
    "rxjs" : "^6.4.0",
    "tslint" : "5.7.0",
    "gulp-tslint" : "8.1.3",
    "typescript" : "2.7.2",
    "awesome-typescript-loader" : "5.0.0",
    "codelyzer" : "4.2.1",
    "@angular/animations" : "^7.2.9",
    "@types/jest": "22.2.3",
    "jest": "22.4.3",
    "jest-preset-angular": "5.2.2",
    "@types/node": "8.0.44",
    "@types/lodash": "4.14.92",
    "angular-cli-ghpages": "0.5.2",
    "angular2-template-loader": "0.6.2",
    "@compodoc/gulp-compodoc": "0.0.8",
    "commitplease": "3.2.0",
    "conventional-github-releaser": "2.0.2",
    "core-js": "2.5.5",
    "del": "3.0.0",
    "greenkeeper-lockfile": "1.14.0",
    "gulp": "3.9.1",
    "gulp-bump": "3.1.1",
    "gulp-conventional-changelog": "1.1.24",
    "gulp-coveralls": "0.1.4",
    "gulp-file": "0.4.0",
    "gulp-git": "2.6.0",
    "gulp-inline-ng2-template": "4.1.0",
    "gulp-hub": "0.8.0",
    "ansi-colors": "2.0.1",
    "fancy-log": "1.3.2",
    "html-loader": "0.5.5",    
    "pump": "3.0.0",
    "rollup": "0.58.2",
    "rollup-plugin-uglify": "3.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "run-sequence": "2.2.1",
    "autoprefixer": "8.4.1",
    "cssnano": "3.10.0",
    "sass-loader": "7.0.1",
    "css-loader": "0.28.11",
    "to-string-loader": "1.1.5",
    "node-sass": "4.9.0",
    "postcss": "6.0.22",
    "postcss-strip-inline-comments": "0.1.5",
    "raw-loader": "0.5.1",
    "source-map-loader": "0.2.3",
    "travis-status": "2.0.0",
    "lodash": "4.17.10",
    "yargs": "11.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [ "<rootDir>/src/" ],
    "coverageReporters": [ "lcov", "text" ],
    "coveragePathIgnorePatterns": [ "/node_modules/", "/config", "/dist/"],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@types/jasmine"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
